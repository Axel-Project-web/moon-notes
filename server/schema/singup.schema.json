{
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "pattern": "^[a-z0-9A-Z._]+@[A-Z0-9a-z_.]+$"
    },
    "password": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9._]+$"
    },
    "phone": {
      "type": "string",
      "pattern": "^[0-9]{11,14}$"
    }
  },
  "additionalProperties": false,
  "required": ["email", "password", "phone"]
}
